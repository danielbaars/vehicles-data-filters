{
  "extends": "airbnb",
    "plugins": [
      "jsx-a11y"
    ],
  "env": {
    "jest": true
  },
  "rules": {
    "react/jsx-filename-extension": 0,
    "jsx-quotes": 0,
    "object-curly-newline": 0,
    "import/prefer-default-export": 0,
    "import/extensions": 0,
    "max-len": 0,
    "react/forbid-prop-types": 0,
    "arrow-body-style": 0,
    "jsx-a11y/href-no-hash": "off",
    "jsx-a11y/anchor-is-valid": ["warn", { "aspects": ["invalidHref"] }]
  }
}
